{
  "title": "ApiClientLight",
  "version": "1.0.1-beta2",
  "description": "Simple library for requesting Api in Json.",
  "authors": [ "maxfoow" ],
  "packOptions": {
    "owners": [ "maxfoow" ],
    "tags": [ "Http", "Json", "HttpClient", "ApiClient" ],
    "projectUrl": "https://github.com/maxfoow/JsonApiClientLight",
    "licenseUrl": "https://github.com/maxfoow/JsonApiClientLight/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/maxfoow/JsonApiClientLight"
    }
  },
  "dependencies": {
    "Newtonsoft.Json": "8.0.3"
  },
  "frameworks": {
    "netstandard1.1": {
      "imports": [ "dnxcore50", "dotnet5.6", "portable-net452+win81", "uap10" ],
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-*",
        "System.Dynamic.Runtime": "4.0.0"
      }
    }
  },
  "scripts": {
    "postcompile": [
      "dotnet pack --no-build --configuration %compile:Configuration%"
    ]
  }
